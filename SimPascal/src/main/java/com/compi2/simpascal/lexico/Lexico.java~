            }
          // fall through
          case 109: break;
          case 41:
            { return new Symbol(sym.NOT, yyline, yycolumn, yytext());
            }
          // fall through
          case 110: break;
          case 42:
            { return new Symbol(sym.SET, yyline, yycolumn, yytext());
            }
          // fall through
          case 111: break;
          case 43:
            { return new Symbol(sym.VAR, yyline, yycolumn, yytext());
            }
          // fall through
          case 112: break;
          case 44:
            { return new Symbol(sym.CHAR, yyline, yycolumn, yytext());
            }
          // fall through
          case 113: break;
          case 45:
            { return new Symbol(sym.ELSE, yyline, yycolumn, yytext());
            }
          // fall through
          case 114: break;
          case 46:
            { return new Symbol(sym.FILE, yyline, yycolumn, yytext());
            }
          // fall through
          case 115: break;
          case 47:
            { return new Symbol(sym.GOTO, yyline, yycolumn, yytext());
            }
          // fall through
          case 116: break;
          case 48:
            { return new Symbol(sym.REAL, yyline, yycolumn, yytext());
            }
          // fall through
          case 117: break;
          case 49:
            { return new Symbol(sym.THEN, yyline, yycolumn, yytext());
            }
          // fall through
          case 118: break;
          case 50:
            { return new Symbol(sym.TYPE, yyline, yycolumn, yytext());
            }
          // fall through
          case 119: break;
          case 51:
            { return new Symbol(sym.WITH, yyline, yycolumn, yytext());
            }
          // fall through
          case 120: break;
          case 52:
            { return new Symbol(sym.ARRAY, yyline, yycolumn, yytext());
            }
          // fall through
          case 121: break;
          case 53:
            { return new Symbol(sym.BEGIN, yyline, yycolumn, yytext());
            }
          // fall through
          case 122: break;
          case 54:
            { return new Symbol(sym.CONST, yyline, yycolumn, yytext());
            }
          // fall through
          case 123: break;
          case 55:
            { return new Symbol(sym.LABEL, yyline, yycolumn, yytext());
            }
          // fall through
          case 124: break;
          case 56:
            { return new Symbol(sym.UNTIL, yyline, yycolumn, yytext());
            }
          // fall through
          case 125: break;
          case 57:
            { return new Symbol(sym.WHILE, yyline, yycolumn, yytext());
            }
          // fall through
          case 126: break;
          case 58:
            { return new Symbol(sym.DOWNTO, yyline, yycolumn, yytext());
            }
          // fall through
          case 127: break;
          case 59:
            { return new Symbol(sym.PACKED, yyline, yycolumn, yytext());
            }
          // fall through
          case 128: break;
          case 60:
            { return new Symbol(sym.READLN, yyline, yycolumn, yytext());
            }
          // fall through
          case 129: break;
          case 61:
            { return new Symbol(sym.RECORD, yyline, yycolumn, yytext());
            }
          // fall through
          case 130: break;
          case 62:
            { return new Symbol(sym.REPEAT, yyline, yycolumn, yytext());
            }
          // fall through
          case 131: break;
          case 63:
            { return new Symbol(sym.STRING, yyline, yycolumn, yytext());
            }
          // fall through
          case 132: break;
          case 64:
            { return new Symbol(sym.BOOLEAN, yyline, yycolumn, yytext());
            }
          // fall through
          case 133: break;
          case 65:
            { return new Symbol(sym.INTEGER, yyline, yycolumn, yytext());
            }
          // fall through
          case 134: break;
          case 66:
            { return new Symbol(sym.PROGRAM, yyline, yycolumn, yytext());
            }
          // fall through
          case 135: break;
          case 67:
            { return new Symbol(sym.WRITELN, yyline, yycolumn, yytext());
            }
          // fall through
          case 136: break;
          case 68:
            { return new Symbol(sym.FUNCTION, yyline, yycolumn, yytext());
            }
          // fall through
          case 137: break;
          case 69:
            { return new Symbol(sym.PROCEDURE, yyline, yycolumn, yytext());
            }
          // fall through
          case 138: break;
          default:
            zzScanError(ZZ_NO_MATCH);
        }
      }
    }
  }


}
